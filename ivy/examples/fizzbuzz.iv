  n0 = @n32_rem(10 @n32_add(48 digit))
  n1 = @n32_div(10 dup(rest0 rest1))
  rest0 = ?(
    ::num::end
    ::num::loop
    fn(ref(io0 io1) fn(rest1 _))
  )
  io1 = @io_print_byte(digit io2)
}

