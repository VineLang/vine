
pub trait[A, B] BitAnd[O] {
  #[builtin = "and"]
  fn bit_and(a: A, b: B) -> O;
}

pub trait[A, B] BitOr[O] {
  #[builtin = "or"]
  fn bit_or(a: A, b: B) -> O;
}

pub trait[A, B] BitXor[O] {
  #[builtin = "xor"]
  fn bit_xor(a: A, b: B) -> O;
}

pub trait[A, B] Shl[O] {
  #[builtin = "shl"]
  fn shl(a: A, b: B) -> O;
}

pub trait[A, B] Shr[O] {
  #[builtin = "shr"]
  fn shr(a: A, b: B) -> O;
}

pub trait[A] Not[O] {
  #[builtin = "not"]
  fn not(a: A) -> O;
}
