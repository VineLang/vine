
use std::numeric::Nat;
use std::debug::Show;

pub fn main(&io: &IO) {
  let edges = [0, 1, N32::maximum - 1, N32::maximum];

  for lo0 in edges.into_iter() {
    for hi0 in edges.into_iter() {
      for lo1 in edges.into_iter() {
        for hi1 in edges.into_iter() {
          let n0 = Nat::from_parts([lo0, hi0]);
          let n1 = Nat::from_parts([lo1, hi1]);
          io.println("{n0} + {n1} = {n0 + n1}");
          io.println("{n0} - {n1} = {n0 - n1}");
          io.println("{n0} * {n1} = {n0 * n1}");
          if n1 != Nat::zero {
            io.println("{n0}.div_rem_n32({lo1}) = {n0.div_rem_n32(lo1).show()}");
          }
        }
      }
    }
  }
}
