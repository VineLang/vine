
use std::{list::String, io::println};

fn main(&io: &IO) {
  let zero_count = 0;
  io.println(foo(0, &zero_count));
  io.println(foo(1, &zero_count));
  io.println(foo(0, &zero_count));
  io.println("\nzero_count: " ++ std::u32::to_string(zero_count));
}

fn foo(value: u32, &zero_count: &u32) -> String {
  if value {
    return "non-zero!";
  }
  zero_count += 1;
  "zero"
}
