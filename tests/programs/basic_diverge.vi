
pub fn main(&io: &IO) {
  let zero_count = 0;
  io.println(foo(0, &zero_count));
  io.println(foo(1, &zero_count));
  io.println(foo(0, &zero_count));
  io.println("\nzero_count: " ++ zero_count.to_string());
}

fn foo(value: u32, &zero_count: &u32) -> String {
  if value {
    return "non-zero!";
  }
  zero_count += 1;
  "zero"
}
