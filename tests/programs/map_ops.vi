
use std::data::Map;

pub fn main(&io: &IO) {
  let map = Map::empty[N32, String];

  map.insert(4, "four");
  map.insert(123456, "one hundred twenty three thousand four hundred fifty six");
  map.insert(1, "one");

  let key = 4;
  io.println("{map.contains_key(&key)}");
  key = 42;
  io.println("{map.contains_key(&key)}");
  key = 123456;
  io.println("{map.contains_key(&key)}");

  let lengths = map.map_values(fn(s: String) { s.len() });
  io.println("{lengths}")
}
