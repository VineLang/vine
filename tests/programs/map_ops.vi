
// @run -d

use #root::data::Map;

pub fn main(&io: &IO) {
  let map = Map::empty[N32, String];

  map.insert(4, "four");
  map.insert(123456, "one hundred twenty three thousand four hundred fifty six");
  map.insert(1, "one");

  io.println("{map.has(&4)}");
  io.println("{map.has(&42)}");
  io.println("{map.has(&123456)}");

  map.keys().for_each(fn* (&k: &N32) { io.println("{k}") });
  map.values().for_each(fn* (&v: &String) { io.println("{v}") });
  io.println("{map.map_values(fn* (&k: &N32, s: String) { k + s.len() }).show()}");

  io.println("{(map ++ Map::empty).show()}");
  io.println("{(Map::empty ++ map).show()}");
  io.println("{(map ++ [(2, "two"), (4, "vier")] as Map).show()}");

  io.println("{map.show()}")
}
