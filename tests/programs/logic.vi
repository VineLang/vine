
pub fn main(&io: &IO) {
  let fn print_bool(bool) {
    io.println(if bool {
      "true"
    } else {
      "false"
    })
  }

  print_bool(true);
  print_bool(true and true);
  print_bool(true or true);
  print_bool(true or false);
  print_bool(false or true);
  print_bool(true impl true);
  print_bool(false impl true);
  print_bool(false impl false);
  print_bool(!false);
  print_bool(!!true);
  io.println("---");
  print_bool(false);
  print_bool(false or false);
  print_bool(true and false);
  print_bool(false and true);
  print_bool(false and false);
  print_bool(true impl false);
  print_bool(!true);
  print_bool(!!false);

  // Test short-circuiting behavior; `safe eraser` is an eraser node,
  // so would erase `io` if it was evaluated.
  io.println("---");
  print_bool(true or safe eraser);
  print_bool(false impl safe eraser);
  print_bool(false and safe eraser);
}
