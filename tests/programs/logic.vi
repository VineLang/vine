
fn main(&io: &IO) {
  print_bool(&io, true);
  print_bool(&io, true && true);
  print_bool(&io, true || true);
  print_bool(&io, true || false);
  print_bool(&io, false || true);
  print_bool(&io, true => true);
  print_bool(&io, false => true);
  print_bool(&io, false => false);
  print_bool(&io, !false);
  print_bool(&io, !!true);
  io.println("---");
  print_bool(&io, false);
  print_bool(&io, false || false);
  print_bool(&io, true && false);
  print_bool(&io, false && true);
  print_bool(&io, false && false);
  print_bool(&io, true => false);
  print_bool(&io, !true);
  print_bool(&io, !!false);

  // Test short-circuiting behavior; `~_` is an eraser node,
  // so would erase `io` if it was evaluated.
  io.println("---");
  print_bool(&io, true || ~_);
  print_bool(&io, false => ~_);
  print_bool(&io, false && ~_);
}

fn print_bool(&io: &IO, bool: bool) {
  io.println(if bool {
    "true"
  } else {
    "false"
  })
}
