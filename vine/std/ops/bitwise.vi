
pub trait And[A, B, O] {
  fn .and(a: A, b: B) -> O;
}

pub trait Or[A, B, O] {
  fn .or(a: A, b: B) -> O;
}

pub trait Xor[A, B, O] {
  fn .xor(a: A, b: B) -> O;
}

pub trait Shl[A, B, O] {
  fn .shl(a: A, b: B) -> O;
}

pub trait Shr[A, B, O] {
  fn .shr(a: A, b: B) -> O;
}

pub trait Not[A, O] {
  fn .not(a: A) -> O;
}
