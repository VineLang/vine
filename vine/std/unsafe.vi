
pub fn duplicate[T](&value: &T) -> T {
  inline_ivy! (x <- value, x1 -> value) -> T {
    x2
    x = dup(x1 x2)
  }
}

pub const eraser[T]: T = inline_ivy! () -> T { _ };

pub fn erase[T](value: T) {
  ~eraser = value;
}

pub fn unreachable[T]() -> T {
  eraser
}

pub fn move[T](&value: &T) -> T {
  let result = value;
  value = eraser;
  result
}
