
const end: u32 = 100;

fn main(&io: &IO) {
  let n = 1;
  while n <= end {
    io.println(message(n));
    n += 1;
  }
}

fn message(n: u32) -> String {
  let r3 = n % 3;
  let r5 = n % 5;
  if r3 {
    if r5 {
      n.to_string()
    } else {
      "Buzz"
    }
  } else {
    if r5 {
      "Fizz"
    } else {
      "FizzBuzz"
    }
  }
}
