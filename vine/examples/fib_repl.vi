
use std::option::Option::{Some, None};

pub fn main(&io: &IO) {
  while io.prompt("> ") is Some(line) {
    let num = u32::parse(line);
    io.println(match num {
      Some(num) => fib(num).to_string(),
      None => "invalid number",
    });
  }
}

pub fn fib(n: u32) -> u32 {
  let a = 0;
  let b = 1;
  while n {
    n -= 1;
    (a, b) = (b, a + b);
  }
  a
}
