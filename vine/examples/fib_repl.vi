
use std::option::Option::{None, Some};

pub fn main(&io: &IO) {
  while io.prompt("> ") is Some(line) {
    let num = N32::parse(line);
    io.println(match num {
      Some(num) { fib(num).to_string() }
      None { "invalid number" }
    });
  }
}

pub fn fib(n: N32) -> N32 {
  let a = 0;
  let b = 1;
  while n != 0 {
    n -= 1;
    (a, b) = (b, a + b);
  }
  a
}
