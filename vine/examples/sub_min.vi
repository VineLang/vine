
pub fn main(&io: &IO) {
  let list = [4, 3, 7, 9];
  sub_min(&list);
  io.println("{list.show()}");
}

pub fn sub_min(&list: &List[N32]) {
  let loop min: N32;
  let min_acc = None[N32]();

  for &val in list.iter_ref() {
    if min_acc is Some(m) impl val < m {
      min_acc = Some(val);
    }

    val -= min;
  }

  min = min_acc.assume();
}
