
use std::option::Option::{None, Some};

pub fn main(&io: &IO) {
  let list = [4, 3, 7, 9];
  sub_min(&list);
  io.println(list.to_string(N32::to_string));
}

pub fn sub_min(&list: &List[N32]) {
  let ~min: N32;
  let min_acc = None[N32];

  let it = list.iter();
  while it.next() is Some(&val) {
    if min_acc is Some(m) => val < m {
      min_acc = Some(val);
    }

    val -= ~min;
  }

  ~min = min_acc.unwrap();
}
